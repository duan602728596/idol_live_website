{
  "name": "idol_live_website",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "dll": "cross-env NODE_ENV=development milktea-esm dll",
    "start": "npm run clean && cross-env NODE_ENV=development NODE_OPTIONS=\"--max_old_space_size=4096\" milktea-esm start",
    "build": "npm run clean && cross-env NODE_ENV=production milktea-esm build",
    "clean": "rimraf dist",
    "eslint": "cross-env COMMITLINT=1 eslint ./ --fix --cache --cache-location .eslintcache --no-warn-ignored",
    "eslint:no-fix": "cross-env COMMITLINT=1 eslint ./ --cache --cache-location .eslintcache --no-warn-ignored",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.5",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "tslib": "^2.8.1",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.23.3",
    "@sweet-milktea/babel-preset-sweet": "^4.4.0",
    "@sweet-milktea/milktea": "^4.4.0",
    "@sweet-milktea/milktea-cli": "^4.1.0",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vue/babel-plugin-jsx": "^1.2.5",
    "@vue/compiler-sfc": "^3.5.12",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.18.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "postcss-loader": "^8.1.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "vue-eslint-parser": "^9.4.3",
    "vue-loader": "^17.4.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,mjs,cjs,ts,tsx,mts,cts}": "cross-env COMMITLINT=1 eslint --cache --cache-location .eslintcache --no-warn-ignored"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
