{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/python"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.html",
      "status": 404
    },
    {
      "src": "/static/(.*)",
      "dest": "dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/boot/wsgi.py"
    }
  ]
}