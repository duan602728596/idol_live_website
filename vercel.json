{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "installCommand": "corepack enable && yarn",
        "buildCommand": "node --run build:vercel"
      }
    },
    {
      "src": "api/boot/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.html",
      "status": 404
    },
    {
      "src": "/static/(.*)",
      "dest": "dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/boot/wsgi.py"
    }
  ]
}