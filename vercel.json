{
  "builds": [
    {
      "src": "src",
      "use": "@vercel/python",
      "config": {
        "installCommand": "python -m pip install -e ."
      }
    },
    {
      "src": "src-frontend",
      "use": "@vercel/node",
      "config": {
        "installCommand": "corepack enable && yarn",
        "buildCommand": "node --run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/boot/boot/wsgi.py"
    }
  ]
}